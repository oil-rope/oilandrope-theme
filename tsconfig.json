{
  "compilerOptions": {
    "target": "ES3",
    "lib": ["DOM", "ES2021"],
    "jsx": "react",
    "module": "ESNext",
    "rootDir": ".",
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"],
      "@Components/*": ["components/*"],
      "@Icons/*": ["icons/*"]
    },
    "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "resolveJsonModule": true
  }
}
